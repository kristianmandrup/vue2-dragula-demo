{
  "name": "xdragula",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Kristian Mandrup <kmandrup@gmail.com>",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "test": "ava",
    "test:tap": "ava --tap | tap-nyan",
    "watch": "npm run test -- --watch",
    "test:e2e": "node test/e2e/runner.js",
    "test:unit": "npm run unit && npm run e2e",
    "fix": "standard --fix",
    "lint": "eslint --fix --ext .js,.vue src test/unit/specs test/e2e/specs",
    "lint:fix": "eslint --fix .js,.vue src test/unit/specs test/e2e/specs"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "lodash": "^4.17.2",
    "seamless-immutable": "^7.1.2",
    "standard": "^9.0.2",
    "vue": "^2.1.8",
    "vue-loader": "^11.3.4",
    "vue-material": "^0.7.1",
    "vue-router": "^2.1.1",
    "vue-style-loader": "^2.0.5",
    "vue-template-compiler": "^2.1.8",
    "vue2-dragula": "kristianmandrup/vue2-dragula#master",
    "xfaker": "^3.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "ava": "^0.18.2",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chromedriver": "^2.21.2",
    "connect-history-api-fallback": "^1.1.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^7.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.0",
    "function-bind": "^1.0.2",
    "html-webpack-plugin": "^2.8.1",
    "http-proxy-middleware": "^0.17.2",
    "inject-loader": "^3.0.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.3",
    "lolex": "^1.4.0",
    "mocha": "^3.1.0",
